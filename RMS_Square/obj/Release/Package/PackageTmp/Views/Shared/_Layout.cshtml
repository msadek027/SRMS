        function buildMenu(divId, items) {
            $.each(items, function (i, data) {
                var li = $('<li class="treeview"><a href="' + this.FormURL + '" ><i class="' + this.MenuImage + '"></i><span>' + this.MenuName + '</span></a>');
                li.appendTo(divId);
                if (this.MenuItemList && this.MenuItemList.length > 0) {
                    var ul = $('<ul class="treeview-menu"></ul>');
                    ul.appendTo(li);
