    <script>
    $(document).ready(function () {

        // Show/hide employee tooltip on hover
        $('#btnAbout').hover(
            function () {
                $('#employeeTooltip').show();
                loadEmployeeData(); // Load data when hovered
            },
            function () {
                setTimeout(function () {
                    if (!$('#employeeTooltip').is(':hover')) {
                        $('#employeeTooltip').hide();
                    }
                }, 500); // Increased delay for smoothness
            }
        );

        // Keep tooltip open if hovering over it
        $('#employeeTooltip').hover(
            function () {
                $(this).show();
            },
            function () {
                $(this).hide();
            }
        );

        // Load employee data from GetAllInfo API
        function loadEmployeeData() {
            $.ajax({
                url: '@Url.Action("GetAllInfo", "EmployeeInfo")',
                type: 'GET',
                dataType: 'json',
                success: function (data) {
                    displayEmployees(data);
                },
                error: function () {
                    $('#employeeList').html('<p>Error loading employee data</p>');
                }
            });
        }

        // Display employee data in tooltip
        function displayEmployees(employees) {
            var html = '<div class="employee-list-container" style="max-height: 400px; overflow-y: auto;">';

            html += '<div class="form-group" style="margin-bottom: 10px;">';
           // html += '<input type="text" id="txtEmployeeSearch" class="form-control input-sm" placeholder="Search employees..." />';
            html += '</div>';

            html += '<div id="employeeItemsContainer">';

            if (employees && employees.length > 0) {
                for (var i = 0; i < employees.length; i++) {
                    var emp = employees[i];
                    html += '<div class="employee-item" data-employee-id="' + emp.ID + '" style="cursor: pointer; padding: 8px; border-bottom: 1px solid #eee;" ' +

                            'data-employee-name="' + emp.EmployeeName + '" ' +
                            'data-designation-name="' + emp.DesignationName + '" ' +
                            'data-department-name="' + emp.DepartmentName + '" ' +
                            'data-company-name="' + emp.CompanyName + '" ' +
                            'data-status="' + emp.Status + '">';
                    html += '<div class="employee-name"><strong>' + (emp.EmployeeName || 'N/A') + '</strong></div>';
                    html += '<div class="employee-details">';
                    html += '</div>';
                    html += '</div>';
                }

                html += '<div class="employee-more" style="border-top: 2px solid #337ab7; margin-top: 10px; padding-top: 8px;">';
                html += '<small class="text-info"><strong>Total: ' + employees.length + ' employees</strong></small>';
                html += '</div>';

            } else {
                html += '<p class="error-text"><i class="glyphicon glyphicon-info-sign"></i> No employees found</p>';
            }

            html += '</div></div>';
            $('#employeeList').html(html);

            // Search functionality
            //$("#txtEmployeeSearch").keyup(function () {
            //    var val = $(this).val().toLowerCase();
            //    $(".employee-item").each(function () {
            //        var employeeCode = $(this).data('employee-code').toLowerCase();
            //        var employeeName = $(this).data('employee-name').toLowerCase();
            //        var designationName = $(this).data('designation-name').toLowerCase();
            //        var departmentName = $(this).data('department-name').toLowerCase();
            //        var companyName = $(this).data('company-name').toLowerCase();
            //        var status = $(this).data('status').toLowerCase();

            //        if (employeeCode.includes(val) ||
            //            employeeName.includes(val) ||
            //            designationName.includes(val) ||
            //            departmentName.includes(val) ||
            //            companyName.includes(val) ||
            //            status.includes(val)) {
            //            $(this).show();
            //        } else {
            //            $(this).hide();
            //        }
            //    });
            //});

            // Double click to navigate to employee form
            $('.employee-item').off('dblclick').on('dblclick', function () {
                var employeeId = $(this).data('employee-id');
                navigateToEmployeeForm(employeeId);
            });
        }

        // Navigate to employee form with employee details loaded
        function navigateToEmployeeForm(employeeId) {
            $.ajax({
                url: '@Url.Action("GetEmployeeById", "EmployeeInfo")', // Use your GetEmployeeById endpoint
                type: 'GET',
                data: { id: employeeId },
                success: function (data) {
                    // Store the employee data temporarily
                    sessionStorage.setItem('currentEmployee', JSON.stringify(data));

                    // Navigate to the form page
                    window.location.href = '/Regulatory/EmployeeInfo/frmEmployeeInfo';
                },
                error: function () {
                    alert('Error loading employee details');
                }
            });
        }

    });
    </script>

