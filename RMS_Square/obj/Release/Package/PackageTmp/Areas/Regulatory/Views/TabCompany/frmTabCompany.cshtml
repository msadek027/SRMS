<script>
    let savedData = {};
    let currentTabId = '';

    // ✅ Collect form data from tab
    function getTabData(container) {
        const data = {};
        $(container).find('input, select, textarea').each(function () {
            const name = $(this).attr('name') || $(this).attr('id');
            if (name) {
                data[name] = $(this).val();
            }
        });
        return data;
    }
    function enableViewMode() {
        $('#modalBody').find('input, select, textarea, button, checkbox').attr('disabled', true);
    }

    // Clear view mode (enable inputs again)
    function disableViewMode() {
        $('#modalBody').find('input, select, textarea, button, checkbox').attr('disabled', false);
    }
    // ✅ Open modal with content from tab ID and set values if saved
    function openTabModal(tabId, title) {

        debugger;
        currentTabId = tabId;

        // Clone live DOM to preserve form values
        const content = $(`#${tabId}`).clone();
    $('#modalBody').html(content);
    $('#modalTitle').text(title);

    const contentTabCommonValue = $("#TabCommonValue").clone();
    $('#modalCommonHeader').html(contentTabCommonValue)
    //$('#modalCommonHeader').find('input, select, textarea, button, checkbox').attr('disabled', true);
    $('#modalCommonHeader').find('button').hide();
    $('#modalCommonHeader').find('#btnSave').attr('disabled', false).show();
    enableViewMode();
    // Refill form fields if saved previously
    if (savedData[tabId]) {
        for (const key in savedData[tabId]) {
            $('#modalBody').find(`[name="${key}"], [id="${key}"]`).val(savedData[tabId][key]);
        }
    }

    $('#tabModal').modal('show');
    }
    function SetMasterDataTab(data, TabPrefix) {
        $.each(data, function (key, value) {
            $('#' + TabPrefix + key).val(value);
        });
    }

</script>
