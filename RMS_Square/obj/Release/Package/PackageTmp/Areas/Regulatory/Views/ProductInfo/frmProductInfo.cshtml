        }).data('kendoGrid');

        $("#txtPopupProductType").keyup(function () {
            var val = $(this).val();
            $("#PopupWindowGridProductType").data("kendoGrid").dataSource.filter({
                logic: "or",
                filters: [
                     { field: "ProductTypeCode", operator: "contains", value: val },
                     { field: "ProductTypeName", operator: "contains", value: val }

