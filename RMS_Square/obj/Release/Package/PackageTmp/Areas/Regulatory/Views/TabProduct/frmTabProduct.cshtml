    // ✅ Collect form data from tab
    function getTabData(container) {
        const data = {};
        $(container).find('input, select, textarea').each(function () {
            const name = $(this).attr('name') || $(this).attr('id');
            if (name) {
                data[name] = $(this).val();
            }
        });
        return data;
    }
    function enableViewMode() {
        $('#modalBody').find('input, select, textarea, button, checkbox').attr('disabled', true);
    }

    // Clear view mode (enable inputs again)
    function disableViewMode() {
        $('#modalBody').find('input, select, textarea, button, checkbox').attr('disabled', false);
    }
    // ✅ Open modal with content from tab ID and set values if saved
    function openTabModal(tabId, title) {

        debugger;
        currentTabId = tabId;

