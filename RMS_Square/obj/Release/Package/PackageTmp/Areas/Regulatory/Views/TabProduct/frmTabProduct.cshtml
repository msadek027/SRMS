    $('#modalCommonHeader').html(contentTabCommonValue)
    //$('#modalCommonHeader').find('input, select, textarea, button, checkbox').attr('disabled', true);
    $('#modalCommonHeader').find('button').hide();
    $('#modalCommonHeader').find('#btnSave').attr('disabled', false).show();
    enableViewMode();
    // Refill form fields if saved previously
    if (savedData[tabId]) {
        for (const key in savedData[tabId]) {
            $('#modalBody').find(`[name="${key}"], [id="${key}"]`).val(savedData[tabId][key]);
        }
    }

