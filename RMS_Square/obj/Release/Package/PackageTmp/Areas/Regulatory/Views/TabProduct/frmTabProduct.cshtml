        });
        debugger;
        var activeTabIndex = 0;
        var ButtonEvent = '';
        var ButtonEventId = '';

        // 🔁 Retrieve last selected tab label & ID from sessionStorage
        var storedButtonEvent = sessionStorage.getItem('keyOption') || "Product Info";
        var storedButtonEventId = sessionStorage.getItem('keyOptionId') || "tabs-1";

        // Update global variables
        ButtonEvent = storedButtonEvent;
        ButtonEventId = storedButtonEventId;

        // Try to retrieve stored tab index
        if (sessionStorage.getItem('activeTabIndex') !== null) {
            activeTabIndex = parseInt(sessionStorage.getItem('activeTabIndex'), 10);
        }

        // 🔧 Initialize jQuery UI Tabs
        $('#tabs').tabs({
            active: activeTabIndex,
            activate: function (event, ui) {
                // 📦 Get new tab index and ID
                var newActiveTabIndex = $('#tabs').tabs("option", "active");
                var newActiveTabIndexId = $('#tabs > div').eq(newActiveTabIndex).attr('id');

                // 📦 Get new tab label
                var activeTabText = $("#btnList li:nth-child(" + (newActiveTabIndex + 1) + ") a").text();
                var matchingAnchor = $("#btnList a").filter(function () {
                    return $(this).text().trim() === activeTabText;
                });

                // Get the href (e.g., "#tabs-1"), then extract the ID
                var tabHref = matchingAnchor.attr("href"); // e.g., "#tabs-1"
                var tabId = tabHref ? tabHref.replace('#', '') : null;


                // 💾 Store updated values in sessionStorage
                sessionStorage.setItem('activeTabIndex', newActiveTabIndex);
                sessionStorage.setItem('activeTabId', newActiveTabIndexId);

                sessionStorage.setItem('keyOption', activeTabText);
                sessionStorage.setItem('keyOptionId', tabId);

                // 🔁 Update global variables
                ButtonEvent = activeTabText;
                ButtonEventId = newActiveTabIndexId;

                // 📡 Load tab content
                GetView($('#CompanyCode').val(), ButtonEvent);
                CheckPagePartAccessPrivilegeTab($('#CompanyCode').val(), ButtonEvent, "");

                // 🖍️ Highlight the active tab
                $("#btnList li").removeClass('active-tab');
                $("#btnList li:nth-child(" + (newActiveTabIndex + 1) + ")").addClass('active-tab');
            }
        });

        // 🧾 Populate fields from sessionStorage
        $('#CompanyCode').val(sessionStorage.getItem('keyCompanyCode'));

        // 📡 Initial content loading (after everything initialized)
        GetView($('#CompanyCode').val(), ButtonEvent);

        // 🖍️ Mark the initially active tab
        $("#btnList li").removeClass('active-tab');
        $("#btnList li:nth-child(" + (activeTabIndex + 1) + ")").addClass('active-tab');

        // 🖱️ Manual tab click handler (in case user clicks tab header list)
        $("#btnList li").click(function () {
            ButtonEvent = $(this).text();
            var tabHref = $(this).find('a').attr('href'); // e.g., "#tabs-1"
            var tabId = tabHref.replace('#', '');
            ButtonEventId = tabId;

            // 📡 Load tab content again for this click
            GetView($('#CompanyCode').val(), ButtonEvent);

            // 🖍️ Highlight clicked tab
            $("#btnList li").removeClass('active-tab');
            $(this).addClass('active-tab');
        });



