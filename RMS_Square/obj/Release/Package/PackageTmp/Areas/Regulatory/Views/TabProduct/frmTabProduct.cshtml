    function GetPageAccess(URL, CompanyCode, ButtonEvent, RoleId, PageNo, callback) {
        $.ajax({
            url: URL,
            data: JSON.stringify({ RoleId: RoleId, PageNo: PageNo }),
            type: 'POST',
            contentType: 'application/json;',
            dataType: 'json',
            success: function (data) {
                if (data.length > 0) {
                    callback(data[0]); // return the first result
                } else {
                    callback(null);
                }
            },
            error: function () {
                alert('Error occurred!');
                callback(null);
            }
        });
    }
   




